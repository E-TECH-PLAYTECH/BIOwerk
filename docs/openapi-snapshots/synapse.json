{
  "openapi": "3.1.0",
  "info": {
    "title": "Synapse",
    "version": "1.0.0"
  },
  "paths": {
    "/health": {
      "get": {
        "tags": [
          "Health"
        ],
        "summary": "Health check (liveness probe)",
        "description": "Returns the health status of the service. Used by orchestrators to detect if service is alive.",
        "operationId": "health_endpoint_health_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HealthCheckResponse"
                }
              }
            }
          }
        }
      }
    },
    "/ready": {
      "get": {
        "tags": [
          "Health"
        ],
        "summary": "Readiness check (readiness probe)",
        "description": "Returns the readiness status of the service. Used by orchestrators to determine if service can accept traffic.",
        "operationId": "readiness_endpoint_ready_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HealthCheckResponse"
                }
              }
            }
          }
        }
      }
    },
    "/v1/storyboard": {
      "post": {
        "summary": "Storyboard V1",
        "description": "Storyboard endpoint (API v1).",
        "operationId": "storyboard_v1_v1_storyboard_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Msg"
              },
              "examples": {
                "launch_review": {
                  "summary": "Outline a launch review deck",
                  "value": {
                    "origin": "swagger-ui",
                    "target": "synapse",
                    "intent": "storyboard",
                    "input": {
                      "topic": "Q4 launch review",
                      "goal": "Show readiness, risks, and next steps",
                      "audience": "executive steering committee",
                      "num_slides": 8
                    },
                    "api_version": "v1"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Reply"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/slide_make": {
      "post": {
        "summary": "Slide Make V1",
        "description": "Slide Make endpoint (API v1).",
        "operationId": "slide_make_v1_v1_slide_make_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Msg"
              },
              "examples": {
                "expand_storyboard": {
                  "summary": "Draft slides from a storyboard",
                  "value": {
                    "origin": "swagger-ui",
                    "target": "synapse",
                    "intent": "slide_make",
                    "input": {
                      "storyboard": [
                        {
                          "title": "Readiness snapshot",
                          "bullets": [
                            "SLO burn < 0.6",
                            "All runbooks rehearsed"
                          ]
                        },
                        {
                          "title": "Risks + mitigations",
                          "bullets": [
                            "Traffic spike",
                            "Database lag"
                          ]
                        }
                      ],
                      "topic": "BIOwerk platform launch"
                    },
                    "api_version": "v1"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Reply"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/visualize": {
      "post": {
        "summary": "Visualize V1",
        "description": "Visualize endpoint (API v1).",
        "operationId": "visualize_v1_v1_visualize_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Msg"
              },
              "examples": {
                "chart_request": {
                  "summary": "Create a simple chart",
                  "value": {
                    "origin": "swagger-ui",
                    "target": "synapse",
                    "intent": "visualize",
                    "input": {
                      "description": "Weekly signups vs activations",
                      "data": [
                        {
                          "week": "W41",
                          "signups": 420,
                          "activations": 310
                        },
                        {
                          "week": "W42",
                          "signups": 450,
                          "activations": 330
                        }
                      ],
                      "viz_type": "bar"
                    },
                    "api_version": "v1"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Reply"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/export": {
      "post": {
        "summary": "Export  V1",
        "description": "Export  endpoint (API v1).",
        "operationId": "export__v1_v1_export_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Msg"
              },
              "examples": {
                "deck_export": {
                  "summary": "Export a finished slide deck",
                  "value": {
                    "origin": "swagger-ui",
                    "target": "synapse",
                    "intent": "export",
                    "input": {
                      "title": "Launch Readiness Deck",
                      "sections": [
                        {
                          "id": "s1",
                          "title": "Overview",
                          "text": "Launch readiness status by track."
                        },
                        {
                          "id": "s2",
                          "title": "Risks",
                          "text": "Traffic spikes and rollback rehearsal outcomes."
                        }
                      ],
                      "metadata": {
                        "theme": "executive",
                        "cover_image": "s3://artifacts/launch-cover.png"
                      }
                    },
                    "api_version": "v1"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Reply"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
