  "components": {
    "schemas": {
      "ComponentHealth": {
        "properties": {
          "name": {
            "type": "string",
            "title": "Name"
          },
          "status": {
            "$ref": "#/components/schemas/HealthStatus"
          },
          "message": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Message"
          },
          "latency_ms": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "title": "Latency Ms"
          },
          "metadata": {
            "type": "object",
            "title": "Metadata"
          },
          "timestamp": {
            "type": "number",
            "title": "Timestamp"
          }
        },
        "type": "object",
        "required": [
          "name",
          "status"
        ],
        "title": "ComponentHealth",
        "description": "Health status for a single component."
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "HealthCheckResponse": {
        "properties": {
          "status": {
            "$ref": "#/components/schemas/HealthStatus"
          },
          "service": {
            "type": "string",
            "title": "Service"
          },
          "version": {
            "type": "string",
            "title": "Version",
            "default": "1.0.0"
          },
          "timestamp": {
            "type": "number",
            "title": "Timestamp"
          },
          "uptime_seconds": {
            "type": "number",
            "title": "Uptime Seconds"
          },
          "checks": {
            "items": {
              "$ref": "#/components/schemas/ComponentHealth"
            },
            "type": "array",
            "title": "Checks"
          },
          "metadata": {
            "type": "object",
            "title": "Metadata"
          }
        },
        "type": "object",
        "required": [
          "status",
          "service",
          "uptime_seconds",
          "checks"
        ],
        "title": "HealthCheckResponse",
        "description": "Complete health check response."
      },
      "HealthStatus": {
        "type": "string",
        "enum": [
          "healthy",
          "unhealthy",
          "degraded",
          "unknown"
        ],
        "title": "HealthStatus",
        "description": "Health check status values."
      },
      "Msg": {
        "properties": {
          "id": {
            "type": "string",
            "title": "Id"
          },
          "ts": {
            "type": "number",
            "title": "Ts"
          },
          "origin": {
            "type": "string",
            "title": "Origin"
          },
          "target": {
            "type": "string",
            "title": "Target"
          },
          "intent": {
            "type": "string",
            "title": "Intent"
          },
          "input": {
            "type": "object",
            "title": "Input",
            "default": {}
          },
          "api_version": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Api Version",
            "description": "API version for this message. Defaults to v1 for backward compatibility.",
