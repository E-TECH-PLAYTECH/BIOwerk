          }
        },
        "type": "object",
        "required": [
          "origin",
          "target",
          "intent"
        ],
        "title": "Msg"
      },
      "Reply": {
        "properties": {
          "id": {
            "type": "string",
            "title": "Id"
          },
          "ts": {
            "type": "number",
            "title": "Ts"
          },
          "agent": {
            "type": "string",
            "title": "Agent"
          },
          "ok": {
            "type": "boolean",
            "title": "Ok"
          },
          "output": {
            "type": "object",
            "title": "Output"
          },
          "state_hash": {
            "type": "string",
            "title": "State Hash"
          },
          "api_version": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Api Version",
            "description": "API version used to generate this reply. Matches the request version.",
            "default": "v1"
          }
        },
        "type": "object",
        "required": [
          "id",
          "ts",
          "agent",
          "ok",
          "output",
          "state_hash"
        ],
        "title": "Reply"
      },
      "ValidationError": {
        "properties": {
