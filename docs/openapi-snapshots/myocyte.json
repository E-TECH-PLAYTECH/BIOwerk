          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Msg"
              },
              "examples": {
                "csv_ingest": {
                  "summary": "Parse a pasted CSV extract",
                  "value": {
                    "origin": "swagger-ui",
                    "target": "myocyte",
                    "intent": "ingest_table",
                    "input": {
                      "raw_data": "region,revenue,expenses\nNA,120000,45000\nEU,98000,41000",
                      "tables": []
