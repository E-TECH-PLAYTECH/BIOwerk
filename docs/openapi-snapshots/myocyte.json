              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/formula_eval": {
      "post": {
        "summary": "Formula Eval V1",
        "description": "Formula Eval endpoint (API v1).",
        "operationId": "formula_eval_v1_v1_formula_eval_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Msg"
              },
              "examples": {
                "apply_formulas": {
                  "summary": "Calculate gross margin and growth",
                  "value": {
                    "origin": "swagger-ui",
                    "target": "myocyte",
                    "intent": "formula_eval",
                    "input": {
                      "tables": [
                        {
                          "id": "rev-1",
                          "name": "Monthly Revenue",
                          "headers": [
                            "month",
                            "revenue",
                            "expenses"
                          ],
                          "rows": [
                            [
                              "Jan",
                              120000,
                              45000
                            ],
                            [
                              "Feb",
                              135000,
                              47000
                            ],
                            [
                              "Mar",
                              150000,
                              50000
                            ]
                          ]
                        }
                      ],
                      "formulas": [
                        {
                          "name": "gross_margin",
                          "expression": "revenue - expenses"
                        },
                        {
                          "name": "growth_rate",
                          "expression": "(revenue / lag(revenue)) - 1"
                        }
                      ]
                    },
                    "api_version": "v1"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Reply"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/model_forecast": {
      "post": {
        "summary": "Model Forecast V1",
        "description": "Model Forecast endpoint (API v1).",
        "operationId": "model_forecast_v1_v1_model_forecast_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Msg"
              },
              "examples": {
                "forecast_pipeline": {
                  "summary": "Generate a revenue forecast",
                  "value": {
                    "origin": "swagger-ui",
                    "target": "myocyte",
                    "intent": "model_forecast",
                    "input": {
                      "tables": [
                        {
                          "id": "rev-1",
                          "name": "ARR by month",
                          "headers": [
                            "month",
                            "arr"
                          ],
                          "rows": [
                            [
                              "Jan",
                              220000
                            ],
                            [
                              "Feb",
                              230000
                            ],
                            [
                              "Mar",
                              245000
                            ]
                          ]
                        }
                      ],
                      "forecast_type": "trend",
                      "periods": 4
                    },
                    "api_version": "v1"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Reply"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/export": {
      "post": {
        "summary": "Export  V1",
        "description": "Export  endpoint (API v1).",
        "operationId": "export__v1_v1_export_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Msg"
              },
              "examples": {
                "bundle_artifact": {
                  "summary": "Export calculated tables with metadata",
