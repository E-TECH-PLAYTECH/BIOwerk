  "paths": {
    "/health": {
      "get": {
        "tags": [
          "Health"
        ],
        "summary": "Health check (liveness probe)",
        "description": "Returns the health status of the service. Used by orchestrators to detect if service is alive.",
        "operationId": "health_endpoint_health_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HealthCheckResponse"
                }
              }
            }
          }
        }
      }
    },
    "/ready": {
      "get": {
        "tags": [
          "Health"
        ],
        "summary": "Readiness check (readiness probe)",
        "description": "Returns the readiness status of the service. Used by orchestrators to determine if service can accept traffic.",
        "operationId": "readiness_endpoint_ready_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HealthCheckResponse"
                }
              }
            }
          }
        }
      }
    },
    "/v1/plan_timeline": {
      "post": {
        "summary": "Plan Timeline V1",
        "description": "Plan Timeline endpoint (API v1).",
        "operationId": "plan_timeline_v1_v1_plan_timeline_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Msg"
              },
              "examples": {
                "launch_timeline": {
                  "summary": "Create a 10-week rollout plan",
                  "value": {
                    "origin": "swagger-ui",
                    "target": "circadian",
                    "intent": "plan_timeline",
                    "input": {
                      "project_description": "BIOwerk launch hardening",
                      "goals": [
                        "Complete load test at 5x baseline",
                        "Run failover rehearsal with database replicas",
                        "Publish customer-facing status runbook"
                      ],
                      "duration_weeks": 10,
                      "team_size": 6
                    },
                    "api_version": "v1"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Reply"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/assign": {
      "post": {
        "summary": "Assign V1",
        "description": "Assign endpoint (API v1).",
        "operationId": "assign_v1_v1_assign_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Msg"
              },
              "examples": {
                "task_assignment": {
                  "summary": "Assign work by skills and workload",
                  "value": {
                    "origin": "swagger-ui",
                    "target": "circadian",
                    "intent": "assign",
                    "input": {
                      "tasks": [
                        {
                          "id": "t1",
                          "name": "Chaos drill",
                          "priority": "high",
                          "required_skills": [
                            "sre",
                            "observability"
                          ]
                        },
                        {
                          "id": "t2",
                          "name": "Runbook review",
                          "priority": "medium",
                          "required_skills": [
                            "ops",
                            "docs"
                          ]
                        }
                      ],
                      "team_members": [
                        {
                          "name": "Alex",
                          "skills": [
                            "sre",
                            "platform"
                          ],
                          "capacity": 0.5
                        },
                        {
                          "name": "Priya",
                          "skills": [
                            "ops",
                            "docs"
                          ],
                          "capacity": 0.8
                        }
                      ]
                    },
                    "api_version": "v1"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Reply"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/track": {
      "post": {
        "summary": "Track V1",
        "description": "Track endpoint (API v1).",
        "operationId": "track_v1_v1_track_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Msg"
              },
              "examples": {
                "project_status": {
                  "summary": "Assess progress by week",
                  "value": {
                    "origin": "swagger-ui",
                    "target": "circadian",
                    "intent": "track",
                    "input": {
                      "current_week": 4,
                      "completed_tasks": [
                        {
                          "id": "t1",
                          "name": "Chaos drill",
                          "status": "done"
                        }
                      ],
                      "in_progress_tasks": [
                        {
                          "id": "t2",
                          "name": "Runbook review",
                          "status": "in_progress"
                        }
                      ],
                      "timeline": [
                        {
                          "id": "m1",
                          "milestone": "Resilience tests",
                          "week": 3
                        },
                        {
                          "id": "m2",
                          "milestone": "GA checklist",
                          "week": 6
                        }
                      ]
                    },
                    "api_version": "v1"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Reply"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/remind": {
      "post": {
        "summary": "Remind V1",
        "description": "Remind endpoint (API v1).",
        "operationId": "remind_v1_v1_remind_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Msg"
              },
              "examples": {
                "weekly_reminders": {
                  "summary": "Generate reminders from assignments",
                  "value": {
                    "origin": "swagger-ui",
                    "target": "circadian",
                    "intent": "remind",
                    "input": {
                      "current_week": 5,
                      "timeline": [
                        {
                          "id": "m2",
                          "milestone": "GA checklist",
                          "week": 6
                        }
                      ],
                      "assignments": [
                        {
                          "task_id": "t2",
                          "task_name": "Runbook review",
                          "assignee": "Priya",
                          "due_week": 5
                        },
                        {
                          "task_id": "t3",
                          "task_name": "Load test at 5x",
                          "assignee": "Alex",
                          "due_week": 6
                        }
                      ]
                    },
                    "api_version": "v1"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Reply"
                }
